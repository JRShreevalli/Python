// LEFT SHIFT OPPERATION 

#include <iostream>
using namespace std ;

int CheckSetBit( int num , int k )
{
    if( num & ( 1 << k - 1 ))
        return 1 ;
    else
        return 0 ;
}
int main()
{
    int n , k ;
    cin >> n >> k ;
    int res = CheckSetBit( n , k ) ;
    cout << res ;
return 0 ; 
}

